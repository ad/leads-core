{
  "version": "1.0.5",
  "name": "leads-core",
  "slug": "leads-core",
  "description": "",
  "startup": "application",
  "boot": "auto",
  "image": "danielapatin/leads-core",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "hassio_role": "default",
  "map": ["config:rw"],
  "webui": "http://[HOST]:[PORT:7891]/",
  "panel_admin": true,
  "panel_icon": "mdi:home-city-outline",
  "panel_title": "leads core",
  "ports": {
    "7890/tcp": 7891
  },
  "ports_description": {
    "7890/tcp": "Exposed port"
  },
  "hassio_api": true,
  "homeassistant_api": true,
  "host_network": false,
  "options": {
    "SERVER": {
      "PORT": "7891",
      "READ_TIMEOUT": "30s",
      "WRITE_TIMEOUT": "30s"
    },
    "REDIS": {
      "ADDRESSES": ["localhost:6379"],
      "PASSWORD": "",
      "DB": 0,
      "REDKA_PORT": "6379",
      "REDKA_DB_PATH": "file:redka.db"
    },
    "JWT": {
      "SECRET": ""
    },
    "RATE_LIMIT": {
      "IP_PER_MINUTE": 100,
      "GLOBAL_PER_MINUTE": 1000
    },
    "TTL": {
      "FREE_DAYS": 7,
      "PRO_DAYS": 365
    }
  },
  "schema": {
    "SERVER": {
      "PORT": "str",
      "READ_TIMEOUT": "str",
      "WRITE_TIMEOUT": "str"
    },
    "REDIS": {
      "ADDRESSES": "list(str)",
      "PASSWORD": "str",
      "DB": "int",
      "REDKA_PORT": "str",
      "REDKA_DB_PATH": "str"
    },
    "JWT": {
      "SECRET": "str"
    },
    "RATE_LIMIT": {
      "IP_PER_MINUTE": "int",
      "GLOBAL_PER_MINUTE": "int"
    },
    "TTL": {
      "FREE_DAYS": "int",
      "PRO_DAYS": "int"
    }
  }
}
