{
  "version": "1.0.0",
  "name": "leads-core",
  "slug": "leads-core",
  "description": "",
  "startup": "application",
  "boot": "auto",
  "image": "danielapatin/leads-core",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "hassio_role": "default",
  "map": ["config:rw"],
  "webui": "http://[HOST]:[PORT:7891]/",
  "panel_admin": true,
  "panel_icon": "mdi:home-city-outline",
  "panel_title": "leads core",
  "ports": {
    "7890/tcp": 7891
  },
  "ports_description": {
    "7890/tcp": "Exposed port"
  },
  "hassio_api": true,
  "homeassistant_api": true,
  "host_network": false,
  "options": {
		"SERVER_PORT": "7891",
    "SERVER_READ_TIMEOUT": "30s",
    "SERVER_WRITE_TIMEOUT": "30s",
    "REDIS_ADDRESSES": "redka",
    "REDIS_PASSWORD": "",
    "REDIS_DB": 0,
    "REDKA_PORT": "6379",
    "REDKA_DB_PATH": "file:redka.db",
    "JWT_SECRET": "",
    "RATE_LIMIT_IP_PER_MINUTE": 100,
    "RATE_LIMIT_GLOBAL_PER_MINUTE": 1000,
    "TTL_FREE_DAYS": 7,
    "TTL_PRO_DAYS": 365
	},
  "schema": {
    "SERVER_PORT": "str",
    "SERVER_READ_TIMEOUT": "str",
    "SERVER_WRITE_TIMEOUT": "str",
    "REDIS_ADDRESSES": "str",
    "REDIS_PASSWORD": "str",
    "REDIS_DB": "int",
    "REDKA_PORT": "str",
    "REDKA_DB_PATH": "str",
    "JWT_SECRET": "str",
    "RATE_LIMIT_IP_PER_MINUTE": "int",
    "RATE_LIMIT_GLOBAL_PER_MINUTE": "int",
    "TTL_FREE_DAYS": "int",
    "TTL_PRO_DAYS": "int"
  }
}
